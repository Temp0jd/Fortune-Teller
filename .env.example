# AI Provider Configuration
# Using Anthropic-compatible API format
# Copy this file to .env.local and fill in your actual API keys

# === Anthropic/Kimi Configuration (Primary) ===
ANTHROPIC_AUTH_TOKEN=your_anthropic_api_key_here
ANTHROPIC_BASE_URL=https://api.kimi.com/coding/
ANTHROPIC_MODEL=kimi-k2.5
ANTHROPIC_DEFAULT_HAIKU_MODEL=kimi-k2.5
ANTHROPIC_DEFAULT_OPUS_MODEL=kimi-k2.5
ANTHROPIC_DEFAULT_SONNET_MODEL=kimi-k2.5
ANTHROPIC_REASONING_MODEL=kimi-k2.5

# === GLM Configuration (Alternative) ===
GLM_API_KEY=your_glm_api_key_here
GLM_BASE_URL=https://open.bigmodel.cn/api/paas/v4
GLM_MODEL=glm-4.7

# === DeepSeek Configuration ===
DEEPSEEK_API_KEY=your_deepseek_api_key_here
DEEPSEEK_BASE_URL=https://api.deepseek.com
DEEPSEEK_MODEL=deepseek-chat

# Legacy compatibility
AI_PROVIDER=deepseek
KIMI_API_KEY=your_kimi_api_key_here
KIMI_BASE_URL=https://api.kimi.com/coding/
AI_API_KEY=your_ai_api_key_here
AI_BASE_URL=https://api.kimi.com/coding/

# Feature flags
ENABLE_HOROSCOPE=true
ENABLE_TAROT=true
ENABLE_BAZI=true
ENABLE_QIMEN=true
ENABLE_LIUYAO=true
ENABLE_SYNASTRY=true
